 {% load mathfilters %}
 <ENVELOPE>
	<HEADER>
		<TALLYREQUEST>Import Data</TALLYREQUEST>
	</HEADER>
	<BODY>
		<IMPORTDATA>
			<REQUESTDESC>
				    <REPORTNAME>All Masters</REPORTNAME>
                <STATICVARIABLES>
                    <SVCURRENTCOMPANY>{{company.legal_name}}</SVCURRENTCOMPANY>
                </STATICVARIABLES>
			</REQUESTDESC>
			<REQUESTDATA>
                {% for data in bh_heads %}
				<TALLYMESSAGE xmlns:UDF="TallyUDF">
                    <STOCKITEM Action="Create">
                        <NAME>{{data.billing_head}}</NAME>
                            <BASEUNITS>{{uom.name}}</BASEUNITS>
                            <NAME.LIST TYPE="String">
                                <NAME>{{data.billing_head}}</NAME>
                            </NAME.LIST>
                            <GSTTYPEOFSUPPLY>Services</GSTTYPEOFSUPPLY>
                            <GSTAPPLICABLE>Applicable</GSTAPPLICABLE>
                            <VATAPPLICABLE>Applicable</VATAPPLICABLE>
                            <SALESTAXCESSAPPPLICABLE />
                            <GSTDETAILS.LIST>
                                <CALCULATIONTYPE>On Value</CALCULATIONTYPE>
                                <HSNCODE>{{data.hsn_code}}</HSNCODE>
                                <TAXABILITY>Taxable</TAXABILITY>
                                <STATEWISEDETAILS.LIST>
                                    <STATENAME>Any</STATENAME>
                                    <RATEDETAILS.LIST>
                                        <GSTRATEDUTYHEAD>Central Tax</GSTRATEDUTYHEAD>
                                        <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
                                        <GSTRATE>{{ data.gst|div:2 }}</GSTRATE>
                                    </RATEDETAILS.LIST>
                                    <RATEDETAILS.LIST>
                                        <GSTRATEDUTYHEAD>State Tax</GSTRATEDUTYHEAD>
                                        <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
                                        <GSTRATE>{{ data.gst|div:2 }}</GSTRATE>
                                    </RATEDETAILS.LIST>
                                    <RATEDETAILS.LIST>
                                        <GSTRATEDUTYHEAD>Integrated Tax</GSTRATEDUTYHEAD>
                                        <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
                                        <GSTRATE>{{ data.gst }}</GSTRATE>
                                    </RATEDETAILS.LIST>
                                    <RATEDETAILS.LIST>
                                        <GSTRATEDUTYHEAD>Cess</GSTRATEDUTYHEAD>
                                        <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
                                       
                                    </RATEDETAILS.LIST>
                                    <RATEDETAILS.LIST>
                                        <GSTRATEDUTYHEAD>Cess on Qty</GSTRATEDUTYHEAD>
                                        <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
                                       
                                    </RATEDETAILS.LIST>
                                    <RATEDETAILS.LIST>
                                        <GSTRATEDUTYHEAD>State Cess</GSTRATEDUTYHEAD>
                                        <GSTRATEVALUATIONTYPE>Based on Value</GSTRATEVALUATIONTYPE>
                                       
                                    </RATEDETAILS.LIST>
                                </STATEWISEDETAILS.LIST>
                            </GSTDETAILS.LIST>
                    </STOCKITEM>
                </TALLYMESSAGE>
                {% endfor %}
			</REQUESTDATA>
		</IMPORTDATA>
	</BODY>
</ENVELOPE>